<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.genio10">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Genio10">
        <activity
            android:name=".Criterios"
            android:label="Criterios de Búsqueda"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Splash_prueba"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Genio10.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".resultados"
            android:label="Resultados Actividad"
            android:screenOrientation="portrait" />
        <activity
            android:name=".actualizacion_registro"
            android:label="Editar Registro"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity_googleprueba"
            android:label="Inicio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity_menu"
            android:label="Genio Matemático"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Progreso_admin"
            android:label="Progreso de Actividades"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Prueba"
            android:label="Administración de Usuarios"
            android:screenOrientation="portrait" />
        <activity
            android:name=".explicacion_conceptos"
            android:label="Introducción"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ejemplo_conceptos"
            android:label="Ejemplos"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".ejercicio_conceptos"
            android:label="Ejercicios"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Genio10.NoActionBar">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>